<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/margen_8_layout"
    tools:context="com.example.jblandii.protectora.fragments.ProtectoraFragment">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_recycler_protectora"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_filtrar_protectora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="@dimen/margen_8_layout"
        android:layout_marginRight="@dimen/margen_8_layout"
        android:backgroundTint="@color/colorPrimary"
        android:tint="@color/secondaryTextColor"
        app:backgroundTint="@color/colorPrimary"
        app:srcCompat="@drawable/ic_filter" />

</FrameLayout>
